{
  "name": "saas_dashboard",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "sequelize-cli --require ts-node/register db:migrate --config src/config/config.ts",
    "migrate:undo": "sequelize-cli --require ts-node/register db:migrate:undo --config src/config/config.ts",
    "db:migrate": "sequelize-cli db:migrate --config src/config/config.ts --migrations-path migrations --models-path models",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all --config src/config/config.ts --migrations-path migrations --models-path models"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.9",
    "@types/winston": "^2.4.4",
    "sequelize-cli": "^6.6.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.17.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "winston": "^3.19.0"
  }
